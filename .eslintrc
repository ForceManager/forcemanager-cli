{
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "curly": 0,
        "comma-dangle": [2, "always-multiline"],
        "comma-spacing": 0,
        "eqeqeq": [2, "allow-null"],
        "key-spacing": 0,
        "no-underscore-dangle": 0,
        "no-unused-expressions": 0,
        "no-shadow": 0,
        "no-shadow-restricted-names": 0,
        "no-extend-native": 0,
        "no-var": 2,
        "new-cap": 0,
        "quotes": 0,
        "semi-spacing": 0,
        "space-unary-ops": 0,
        "space-infix-ops": 0,
        "consistent-return": 0,
        "strict": 0,
        "jest/valid-expect": 0
    },
    "settings": {
        "propWrapperFunctions": [
            "forbidExtraProps",
            {
                "property": "freeze",
                "object": "Object"
            },
            {
                "property": "myFavoriteWrapper"
            }
        ]
    },
    "globals": {
        "window": true,
        "document": true,
        "localStorage": true,
        "FormData": true,
        "FileReader": true,
        "Blob": true,
        "navigator": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": ["prettier"]
}
